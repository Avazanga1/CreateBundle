{
    "name": "symfony-cmf/create-bundle",
    "type": "symfony-bundle",
    "description": "VIE is a way to make any site using RDFa editable.",
    "license": "MIT",
    "authors": [
        {
            "name": "Liip AG",
            "homepage": "http://www.liip.ch/"
        },
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/SimpleCmsBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "2.1.*",
        "symfony/assetic-bundle": "2.1.*",
        "symfony-cmf/core-bundle": "1.0.*",
        "friendsofsymfony/rest-bundle": "*",
        "midgard/createphp": "dev-master"
    },
    "suggest": {
        "dms/dms-filter": "if you want to filter content on your fields"
    },
    "autoload": {
        "psr-0": { "Symfony\\Cmf\\Bundle\\CreateBundle": "" }
    },
    "target-dir": "Symfony/Cmf/Bundle/CreateBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
